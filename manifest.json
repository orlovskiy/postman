{
  "manifest_version": 2,
  "name": "postman",
  "version": "1.0",

"content_scripts": [
    {
      "matches": [ "*://*/*"],
      "js": [ "content.js" ]
    }
  ],

"permissions":
    [ "contextMenus", "tabs", "activeTab", "storage" , "http://vk.com/*", "https://vk.com/*" ],
"browser_action": {
      "default_title": "take a look on",
      "default_icon": {
        "19":"icon.png"
        },
      "default_popup": "popup.html"
    }
}